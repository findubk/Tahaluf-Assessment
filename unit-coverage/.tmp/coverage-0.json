],"sourcesContent":["import React, { useEffect, useState } from 'react';\n\nimport List from 'src/components/List/List';\nimport Detail from '../Detail/Detail';\n\nimport { fetchList } from 'src/modules/list.module';\nimport Filters from 'src/components/Filters/Filters';\nimport Layout from 'src/layout/Layout';\n\nfunction Home() {\n  const [universitiesList, setUniversitiesList] = useState([]);\n  const [filteredUniversities, setFilteredUniversitiesList] = useState([]);\n  const [selectedUniversity, setSelectedUniversity] = useState(null);\n  const [error, setError] = useState('');\n  const [loading, setLoading] = useState(true);\n\n  const COLUMNS = [\n    { name: 'name', dataKey: 'name' },\n    { name: 'country', dataKey: 'country' },\n    {\n      name: 'action',\n      click: deleteItem => setUniversitiesList(filteredUniversities.filter(item => item.name !== deleteItem.name)),\n    },\n  ];\n\n  const fetchUniversitiesList = async () => {\n    setLoading(true);\n    const response = await fetchList();\n    if (response.success) {\n      setUniversitiesList(response.data);\n      setLoading(false);\n    } else {\n      setError(response.error);\n      setLoading(false);\n    }\n  };\n\n  const showUniversityDetail = university => {\n    setSelectedUniversity(university);\n  };\n\n  useEffect(() => {\n    fetchUniversitiesList();\n  }, []);\n\n  useEffect(() => {\n    setFilteredUniversitiesList(universitiesList);\n  }, [universitiesList]);\n\n  return (\n    <Layout pageHeading={selectedUniversity ? 'University Detail' : 'University List'}>\n      {selectedUniversity ? (\n        <Detail selectedUniversity={selectedUniversity}></Detail>\n      ) : (\n        <>\n          <Filters data={universitiesList} setFilteredData={setFilteredUniversitiesList} />\n          <List\n            data={filteredUniversities}\n            columns={COLUMNS}\n            loading={loading}\n            error={error}\n            onSelectListItem={showUniversityDetail}\n          />\n        </>\n      )}\n    </Layout>\n  );\n}\n\nexport default Home;\n"],"mappings":"AAoDQ,SAEA,UAFA;AApDR,OAAO,SAAS,WAAW,gBAAgB;AAE3C,OAAO,UAAU;AACjB,OAAO,YAAY;AAEnB,SAAS,iBAAiB;AAC1B,OAAO,aAAa;AACpB,OAAO,YAAY;AAEnB,SAAS,OAAO;AACd,QAAM,CAAC,kBAAkB,mBAAmB,IAAI,SAAS,CAAC,CAAC;AAC3D,QAAM,CAAC,sBAAsB,2BAA2B,IAAI,SAAS,CAAC,CAAC;AACvE,QAAM,CAAC,oBAAoB,qBAAqB,IAAI,SAAS,IAAI;AACjE,QAAM,CAAC,OAAO,QAAQ,IAAI,SAAS,EAAE;AACrC,QAAM,CAAC,SAAS,UAAU,IAAI,SAAS,IAAI;AAE3C,QAAM,UAAU;AAAA,IACd,EAAE,MAAM,QAAQ,SAAS,OAAO;AAAA,IAChC,EAAE,MAAM,WAAW,SAAS,UAAU;AAAA,IACtC;AAAA,MACE,MAAM;AAAA,MACN,OAAO,gBAAc,oBAAoB,qBAAqB,OAAO,UAAQ,KAAK,SAAS,WAAW,IAAI,CAAC;AAAA,IAC7G;AAAA,EACF;AAEA,QAAM,wBAAwB,YAAY;AACxC,eAAW,IAAI;AACf,UAAM,WAAW,MAAM,UAAU;AACjC,QAAI,SAAS,SAAS;AACpB,0BAAoB,SAAS,IAAI;AACjC,iBAAW,KAAK;AAAA,IAClB,OAAO;AACL,eAAS,SAAS,KAAK;AACvB,iBAAW,KAAK;AAAA,IAClB;AAAA,EACF;AAEA,QAAM,uBAAuB,gBAAc;AACzC,0BAAsB,UAAU;AAAA,EAClC;AAEA,YAAU,MAAM;AACd,0BAAsB;AAAA,EACxB,GAAG,CAAC,CAAC;AAEL,YAAU,MAAM;AACd,gCAA4B,gBAAgB;AAAA,EAC9C,GAAG,CAAC,gBAAgB,CAAC;AAErB,SACE,uBAAC,UAAO,aAAa,qBAAqB,sBAAsB,mBAC7D,+BACC,uBAAC,UAAO,sBAAR;AAAA;AAAA;AAAA;AAAA,SAAgD,IAEhD,mCACE;AAAA,2BAAC,WAAQ,MAAM,kBAAkB,iBAAiB,+BAAlD;AAAA;AAAA;AAAA;AAAA,WAA+E;AAAA,IAC/E;AAAA,MAAC;AAAA;AAAA,QACC,MAAM;AAAA,QACN,SAAS;AAAA,QACT;AAAA,QACA;AAAA,QACA,kBAAkB;AAAA;AAAA,MALpB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAMA;AAAA,OARF;AAAA;AAAA;AAAA;AAAA,SASA,KAbJ;AAAA;AAAA;AAAA;AAAA,SAeA;AAEJ;AAEA,eAAe;","names":[]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"7a7d7d49d82b2db7cf075d897023ebc0de8719f1"}}