Input/Input';\nimport Select from '../Select/Select';\n\nfunction Filters({ data, setFilteredData }) {\n  const [searchText, setSearchText] = useState('');\n  const [sort, setSort] = useState('asc');\n\n  const filterData = () => {\n    if (data.length) {\n      let filteredData = data.filter(item => {\n        return item.name.toLowerCase().includes(searchText.toLowerCase());\n      });\n\n      if (sort === 'asc') {\n        filteredData = filteredData.sort((a, b) => a.name.localeCompare(b.name));\n      } else {\n        filteredData = filteredData.sort((a, b) => b.name.localeCompare(a.name));\n      }\n\n      setFilteredData(filteredData);\n    }\n  };\n\n  useEffect(() => {\n    filterData();\n  }, [searchText, sort]);\n\n  return (\n    <div className='filter-container'>\n      <Input placeholder={'Search keyword'} value={searchText} onChange={e => setSearchText(e.target.value)}></Input>\n      <Select\n        value={sort}\n        onChange={e => setSort(e.target.value)}\n        options={[\n          {\n            name: 'Sort by name (A-Z)',\n            value: 'asc',\n          },\n          {\n            name: 'Sort by name (Z-A)',\n            value: 'dec',\n          },\n        ]}\n      ></Select>\n    </div>\n  );\n}\n\nexport default Filters;\n"],"mappings":"AAgCM;AAhCN,OAAO,SAAS,WAAW,gBAAgB;AAE3C,OAAO;AACP,OAAO,WAAW;AAClB,OAAO,YAAY;AAEnB,SAAS,QAAQ,EAAE,MAAM,gBAAgB,GAAG;AAC1C,QAAM,CAAC,YAAY,aAAa,IAAI,SAAS,EAAE;AAC/C,QAAM,CAAC,MAAM,OAAO,IAAI,SAAS,KAAK;AAEtC,QAAM,aAAa,MAAM;AACvB,QAAI,KAAK,QAAQ;AACf,UAAI,eAAe,KAAK,OAAO,UAAQ;AACrC,eAAO,KAAK,KAAK,YAAY,EAAE,SAAS,WAAW,YAAY,CAAC;AAAA,MAClE,CAAC;AAED,UAAI,SAAS,OAAO;AAClB,uBAAe,aAAa,KAAK,CAAC,GAAG,MAAM,EAAE,KAAK,cAAc,EAAE,IAAI,CAAC;AAAA,MACzE,OAAO;AACL,uBAAe,aAAa,KAAK,CAAC,GAAG,MAAM,EAAE,KAAK,cAAc,EAAE,IAAI,CAAC;AAAA,MACzE;AAEA,sBAAgB,YAAY;AAAA,IAC9B;AAAA,EACF;AAEA,YAAU,MAAM;AACd,eAAW;AAAA,EACb,GAAG,CAAC,YAAY,IAAI,CAAC;AAErB,SACE,uBAAC,SAAI,WAAU,oBACb;AAAA,2BAAC,SAAM,aAAa,kBAAkB,OAAO,YAAY,UAAU,OAAK,cAAc,EAA